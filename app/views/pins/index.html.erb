<h1>Pins, Bru</h1>

<% @pins.each do |pin| %>
	<%= image_tag pin.image %>
	<p>
		<%= pin.title %>
		<%= pin.description %>
	</p>
<% end %>

<%= link_to "Back to Pins", pins_path %>
<%= link_to "Back to Boards", user_boards_path(@user) %>
<%= link_to "Create a Pin", new_pin_path %>
<%= link_to "Create a Board", new_user_board_path(@user) %>